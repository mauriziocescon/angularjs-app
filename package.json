{
  "name": "angular-1-x-app",
  "version": "1.0.0",
  "description": "A demo application built on angular 1.x components, Typescript, ES2017, ui-router",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": "=10.15.2",
    "npm": "=6.4.1"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "serve": "webpack-dev-server --d --progress --profile --colors --env.name=dev | npm run json-server-watch",
    "build": "webpack --progress --profile  --colors --env.name=prod",
    "pretest": "webpack --progress --profile --colors --env.name=test",
    "test": "karma start karma.conf.js",
    "preprotractor": "tsc --project e2e-tests | webdriver-manager update",
    "protractor": "protractor protractor.conf.js",
    "test-api": "jest rest-api-tests",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/angular-1-x-app"
  },
  "keywords": [
    "angularjs",
    "bootstrap",
    "cescon",
    "es2017",
    "frisby",
    "json-server",
    "karma",
    "maurizio",
    "protractor",
    "sass",
    "travis",
    "typescript",
    "vendor",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
